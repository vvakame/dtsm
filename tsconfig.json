{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "removeComments": false,
        "experimentalDecorators": true,
        "rootDir": ".",
        "sourceMap": true,
        "declaration": true,
        "listFiles": true,
        "newLine": "LF",
        "noEmitOnError": true,
        "inlineSources": true,
        "noImplicitReturns": true,
        "stripInternal": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./**/*.d.ts",
        "./lib/typings/**/*.d.ts",
        "./typings/**/*.d.ts",
        "!./test-tmp/**/*.ts",
        "!./example/**/*.ts",
        "!./pages/**/*.ts",
        "!./bower_components/**/*.ts",
        "!./node_modules/**/*",
        "./node_modules/typescript/lib/lib.es6.d.ts"
    ],
    "files": [
        "./lib/cli.ts",
        "./lib/incrementalsearch.ts",
        "./lib/index.ts",
        "./lib/manager.ts",
        "./lib/model.ts",
        "./lib/tracker.ts",
        "./lib/utils.ts",
        "./test/cliSpec.ts",
        "./test/indexSpec.ts",
        "./test/utilsSpec.ts",
        "./typings/archy/archy.d.ts",
        "./typings/bundle.d.ts",
        "./typings/empower/empower.d.ts",
        "./typings/insight/insight.d.ts",
        "./typings/mkdirp/mkdirp.d.ts",
        "./typings/mocha/mocha.d.ts",
        "./typings/nexpect/nexpect.d.ts",
        "./typings/node/node.d.ts",
        "./typings/power-assert-formatter/power-assert-formatter.d.ts",
        "./typings/power-assert/power-assert.d.ts",
        "./typings/rimraf/rimraf.d.ts",
        "./typings/update-notifier/update-notifier.d.ts",
        "./typings/which/which.d.ts",
        "./node_modules/typescript/lib/lib.es6.d.ts"
    ]
}